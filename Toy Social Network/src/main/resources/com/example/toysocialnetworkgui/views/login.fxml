<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="416.0" prefWidth="670.0" stylesheets="@../css/login.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.toysocialnetworkgui.LoginController">
   <children>
      <BorderPane layoutX="-1.0" layoutY="2.0" prefHeight="410.0" prefWidth="661.0" style="-fx-background-color: #fff;">
         <left>
            <Pane prefHeight="410.0" prefWidth="297.0" styleClass="linear-grad" stylesheets="@../css/login.css" BorderPane.alignment="CENTER">
               <children>
                  <HBox layoutX="14.0" layoutY="14.0" prefHeight="117.0" prefWidth="115.0" />
                  <ImageView fitHeight="177.0" fitWidth="191.0" layoutX="60.0" layoutY="93.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/logo.png" />
                     </image>
                  </ImageView>
               </children>
            </Pane>
         </left>
         <center>
            <StackPane prefHeight="416.0" prefWidth="379.0" BorderPane.alignment="CENTER">
               <children>
                  <Pane fx:id="painRight" prefHeight="410.0" prefWidth="387.0">
                     <children>
                        <TextField fx:id="emailTextField" layoutX="60.0" layoutY="139.0" prefHeight="32.0" prefWidth="249.0" promptText="Email" />
                        <Button fx:id="LoginButton" layoutX="120.0" layoutY="248.0" mnemonicParsing="false" onAction="#login" prefHeight="37.0" prefWidth="125.0" styleClass="LoginButton" stylesheets="@../css/login.css" text="Login" textFill="#fbf7f7">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </Button>
                        <Button fx:id="submitButton" layoutX="55.0" layoutY="326.0" mnemonicParsing="false" onAction="#submit" prefHeight="46.0" prefWidth="261.0" styleClass="submitButton" stylesheets="@../css/login.css" text="Create a new account" textFill="WHITE">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </Button>
                        <ImageView fitHeight="90.0" fitWidth="120.0" layoutX="138.0" layoutY="29.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../images/user-experience.png" />
                           </image>
                        </ImageView>
                        <PasswordField fx:id="passwordTextField" layoutX="61.0" layoutY="196.0" prefHeight="34.0" prefWidth="249.0" promptText="Password" />
                     </children>
                  </Pane>
               </children>
            </StackPane>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
