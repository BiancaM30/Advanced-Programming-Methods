<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@../css/raport.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.toysocialnetworkgui.RaportController">
   <children>
      <Button fx:id="raportButton" layoutX="49.0" layoutY="369.0" mnemonicParsing="false" onAction="#clickOnRaport1" prefHeight="33.0" prefWidth="123.0" text="Generate raport 1" />
      <DatePicker fx:id="startPicker" layoutX="77.0" layoutY="105.0" promptText="Start date" />
      <DatePicker fx:id="endPicker" layoutX="347.0" layoutY="105.0" promptText="End date" />
      <Label layoutX="212.0" layoutY="44.0" prefHeight="33.0" prefWidth="151.0" text="     My activity" textFill="#6b6b9f">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label layoutX="60.0" layoutY="318.0" prefHeight="33.0" prefWidth="207.0" text="New friends and messages" textFill="#6b6b9f">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label layoutX="363.0" layoutY="171.0" prefHeight="25.0" prefWidth="167.0" text="New messages from user" textFill="#6b6b9f">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Button layoutX="352.0" layoutY="424.0" mnemonicParsing="false" onAction="#clickOnRaport2" prefHeight="33.0" prefWidth="123.0" text="Generate raport 2" />
      <TableView fx:id="tableView" layoutX="371.0" layoutY="222.0" prefHeight="165.0" prefWidth="151.0">
        <columns>
          <TableColumn fx:id="tableColumnFirstName" prefWidth="75.0" text="FirstName" />
          <TableColumn fx:id="tableColumnLastName" prefWidth="75.0" text="LastName" />
        </columns>
      </TableView>
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="77.0" layoutY="153.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/raport.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
